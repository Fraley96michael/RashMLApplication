<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#56201C1C">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="406dp"
        android:layout_height="374dp"
        app:layout_constraintBottom_toTopOf="@+id/progressChart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:layout_constraintVertical_bias="0.362"
        app:srcCompat="@drawable/ic_undraw_performance_overview_re_mqrq" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/logout"
        android:layout_width="100dp"
        android:layout_height="49dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_corner_view2"
        android:text="LogOut"
        app:backgroundTint="#D2090808"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/editTextTextPersonName"
        android:layout_width="247dp"
        android:layout_height="66dp"
        android:layout_marginStart="12dp"
        android:ems="10"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logout" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/imageRec"
        android:layout_width="159dp"
        android:layout_height="133dp"
        android:background="@drawable/rounded_corner_view2"
        android:backgroundTint="#D2090808"
        android:text="Image Recognition"

        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.604" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/liveFeed"
        android:layout_width="159dp"
        android:layout_height="133dp"
        android:background="@drawable/rounded_corner_view2"
        android:backgroundTint="#D2090808"
        android:text="Live feed recognition"
        android:textColor="@color/white"
        app:cornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.604" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/progressChart"
        android:layout_width="283dp"
        android:layout_height="71dp"
        android:layout_marginBottom="124dp"
        android:background="@drawable/rounded_corner_view2"
        android:backgroundTint="#D2090808"
        android:text="Progress Chart"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/liveFeed"
        app:layout_constraintVertical_bias="0.705" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="82dp"
        android:layout_height="66dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_baseline_person_24"
        app:tint="#171717" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/rashInfo"
        android:layout_width="283dp"
        android:layout_height="71dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rounded_corner_view2"
        android:backgroundTint="#D2090808"
        android:text="Rash info page"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressChart"
        app:layout_constraintVertical_bias="0.14" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/chatButton"
        android:layout_width="100dp"
        android:layout_height="49dp"
        android:background="@drawable/rounded_corner_view2"
        android:backgroundTint="#D2090808"
        android:text="Chat"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rashInfo"
        app:layout_constraintVertical_bias="0.76" />

</androidx.constraintlayout.widget.ConstraintLayout>